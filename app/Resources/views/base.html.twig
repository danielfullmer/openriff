        {% block flashes %}
            {% flashes %}
                <div class="alert alert-{{ type }}">
                    <button type="button" class="close" data-dismiss="alert">&times;</button>
                    {{ message }}
                </div>
            {% endflashes %}
        {% endblock %}

        {% block body %}
            <p>Hello world! This is HTML5 Boilerplate.</p>
        {% endblock body %}

